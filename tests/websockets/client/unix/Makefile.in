PROGNAME = testpovws

SUBDIRS = ..

#CPPFILES = \
#	../testclient.cpp ../client.cpp

OBJS = ../testclient.o ../client.o

DEPLIBS = 

CLEAN =

################################################################
TOP=@top_srcdir@

include @top_srcdir@/makeinclude

XPCXXFLAGS = $(COMMONINC)

OBJECTS = $(CPPFILES:.cpp=.o) $(CFILES:.c=.o)

all::	$(SUBDIRS) 
	@for dir in $(SUBDIRS); do\
		echo "=== making $$dir ===";\
		(cd $$dir; $(MAKE) $(MFLAGS)) || exit 1;\
	done

all::	$(PROGNAME) 

$(PROGNAME): $(OBJS) $(DEPLIBS)
	echo $(CXX) $@ ...
	$(CXX) $(OBJS) -o $@  $(LDFLAGS) $(LDLIBS) $(DEPLIBS)


clean :
	@for dir in $(SUBDIRS); do\
		echo "=== cleaning $$dir ===";\
		(cd $$dir; $(MAKE) $(MFLAGS) clean) || exit 1;\
	done
	-@ rm -f *.o *.do $(PROGNAME) $(CLEAN) core *~ *.bak cmap
	-@ touch .depend

distclean: clean
	@for dir in $(SUBDIRS); do\
		echo "=== distcleaning $$dir ===";\
		(cd $$dir; $(MAKE) $(MFLAGS) distclean) || exit 1;\
	done
	-@ rm -rf autom4te.cache
	-@ rm -f Makefile .depend configure config.log config.status

depend:
	@for dir in $(SUBDIRS); do\
		echo "=== making dependencies in $$dir ===";\
		(cd $$dir; $(MAKE) $(MFLAGS) depend) || exit 1;\
	done
	-@ makedepend $(OSFLAG) $(XPCXXFLAGS) $(FLTKINC) -f .depend $(CPPFILES) $(CFILES)

include .depend

################################################################



